    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug with OpenOCD",
            "cwd": "${workspaceFolder}",
            "executable": "./build/gateway.elf",
            "request": "launch",
            "type": "cortex-debug",
            "showDevDebugOutput": "raw",
            "configFiles": [
                "${workspaceFolder}/tool/xpack-openocd-0.12.0-6/openocd/scripts/interface/stlink.cfg",
                "${workspaceFolder}/tool/xpack-openocd-0.12.0-6/openocd/scripts/target/stm32f1x.cfg"
            ],
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 10
            },
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "gdbPath": "${workspaceFolder}/tool/arm-gnu-toolchain-14.2.rel1-x86_64-arm-none-eabi/bin/arm-none-eabi-gdb"
           
            
        }
       

    ]
}